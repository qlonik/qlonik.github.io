{
  "name": "qlonik.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check": "tsc",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,json,css,md,yml}\"",
    "pretest": "npm run check && npm run lint",
    "posttest": "npm run format",
    "postinstall": "husky install",
    "dev": "next dev",
    "build": "next build && next export && touch out/.nojekyll",
    "start": "next start"
  },
  "dependencies": {
    "@effect-ts/core": "^0.26.1",
    "@emotion/cache": "^11.1.3",
    "@emotion/react": "^11.1.5",
    "@emotion/server": "^11.0.0",
    "@emotion/styled": "^11.1.5",
    "@material-ui/core": "^5.0.0-alpha.26",
    "clsx": "^1.1.1",
    "next": "^10.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{css,md,json}": "prettier --write"
  }
}
