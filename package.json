{
  "name": "qlonik.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build && next export && touch out/.nojekyll",
    "build:watch": "watch --wait=15 'pnpm run build' pages public src",
    "check": "tsc",
    "clean": "rm -rf .next out",
    "dev": "next dev",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,js,json,css,md,yml,mdx}\"",
    "lint": "eslint . --cache --fix --ext .ts,.tsx,.js",
    "postinstall": "husky install",
    "posttest": "npm run format",
    "pretest": "npm run check && npm run lint",
    "serve": "serve out",
    "start": "next start"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@effect-ts/core": "^0.37.5",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@mui/material": "^6.4.1",
    "@mui/material-nextjs": "^6.3.1",
    "@next/mdx": "^15.1.6",
    "clsx": "^1.1.1",
    "next": "^15.1.6",
    "next-compose-plugins": "^2.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "remark-frontmatter": "^2.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^10.2.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.10.8",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "serve": "^11.3.2",
    "typescript": "^5.7.3",
    "watch": "^1.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{css,md,json}": "prettier --write"
  }
}
